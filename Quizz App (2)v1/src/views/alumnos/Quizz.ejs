                                                            <input autocomplete="off" class="form-check-input reactivo"
                                                              type="checkbox" name="respuesta<%= i %>,<%= j %>"
                                                              value="<%= quizz.cuestionario[i].pregunta[j] %>">
                                                            <label for="pregunta">
                                                              <%= quizz.cuestionario[i].pregunta[j] %>
                                                            </label>
                                                            <br>
                                                            <% } %>
                                                        </div>
                                                        <input type="hidden" autocomplete="off" class="contadorItems"
                                                          value="<%= i %>" name="contadorItems<%= i %>">
                                                        <input type="hidden" autocomplete="off" name="respuesta<%= i %>"
                                                          value="multiple">

                                                      </div>

                                                    </div>

                                                  </div>
                                                </div>
                                            </div>
                                          </div>




                                          <% } %>


                                            <input type="hidden" autocomplete="off" name="cuestionario<%= i %>"
                                              value="<%= quizz.cuestionario[i]._id %>">

                                            <!--Termina Pregunta OM-->
