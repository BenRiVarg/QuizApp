  <!--Invalidadndo campos si no existe ninguna pregunta Drag-->

  <% if (pDrag) { %>
    <script>
      backLienzo();
      var preguntasAr = document.getElementsByClassName("tipoAr");

      var replica = preguntasAr[0].querySelectorAll("div.replica")[0];
      var reactivos =<%- JSON.stringify(quizz.cuestionario[0].pregunta[1]) %>;
      var flechas =<%- JSON.stringify(quizz.cuestionario[iteracionLienzo].pregunta[2]) %>;
      for( x in reactivos){

        var strVar="";
        //------strVar += "  <div class=\"reactivoDrop\" id=\""+reactivos[x].id+"\"  ondrop=\"drop(event,this)\">";
        /*
          strVar += "<div class=\"reactivoDrop\" id=\""+reactivos[x].id+"\"  ondrop=\"drop(event,this)\">";
        strVar += "    <div><\/div>";
        strVar += "  <\/div>";
        */

     //   strVar += "  <div class=\"reactivoDrop\" id=\""+reactivos[x].id+"\"  ondrop=\"drop(event,this)\">";
        strVar += "  <div class=\"reactivoDrop\" id=\""+reactivos[x].id+"\"  >";
        strVar += "                                <div class=\"contenidoDrop\">Texto<\/div>";
        strVar += "                                <div class=\"recuadro cajaDrop\"><\/div>";
        strVar += "                              <\/div>";
          
        replica.insertAdjacentHTML("afterBegin",strVar);

        var reactivo=document.getElementById(reactivos[x].id);
          /*
        var reactivo=document.createElement("DIV");
        reactivo.id=reactivos[x].id;
        reactivo.className="reactivoDrop";
        */


        reactivo.style.top=reactivos[x].top+"px";
        reactivo.style.left=reactivos[x].left+"px";
        //insertorReplica.appendChild(reactivo);
        }
      
        for (y in flechas) {

          //Obtención de los Datos para ver que tipo de Flecha es
          var claveFlecha = flechas[y].id
          var tipoFlecha = claveFlecha.substr(0, 3);
          var imagenFlecha = document.createElement("IMG");
          switch (tipoFlecha) {
