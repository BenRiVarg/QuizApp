                                                          <label class="form-check-label text-light fs-4"
                                                            for="pregunta">
                                                            <%= quizz.cuestionario[i].pregunta[j]%>
                                                          </label>
                                                        </div>

                                                        <% } %>
                                                    </div>

                                                  </div>

                                                </div>
                                              </div>
                                          </div>
                                        </div>


