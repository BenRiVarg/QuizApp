 <!-- <form action="/alumnos/correccion/alumno/<%=idAlumno%>" method="POST" id="quizz">-->
  <!--<form  action="/alumnos/correccion/" method="POST" id="quizz">-->
    <form id="quizz" >
    <input type="hidden" name="alumno" value="<%=idAlumno %>">
    <input type="hidden" name="quizz" autocomplete="off" value="<%= quizz._id %>">
