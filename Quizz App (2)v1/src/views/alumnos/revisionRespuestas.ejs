                                                                                    <label class="form-check-label "
                                                                                      for="pregunta">
                                                                                      <%=
                                                                                        quizz.cuestionario[i].pregunta[k]%>
                                                                                    </label>
                                                                                  </div>
                                                                                  <% } %>




                                                                                    <% } %>


                                                                                      <% } %>
                                                                                        <% } %>
                                                                        </div>
                                                                      </div>
                                                                    </div>
                                                                  </div>
