                                                                                      <%}else{%>
                                                                                            <% var respuestaNeutra=true %>
                                                                                                    <% for(var k=0;k<=(quizz.cuestionario[i].respuesta.length-1);k++){%>
                                                                                                      <!--Respuesta Correcta vacía-->
