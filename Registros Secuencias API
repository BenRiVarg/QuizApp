    {
      id: 'HU-dW413R',
      nombre: 'TARJETAS DE IDENTIDAD',
      bloque: 'dSnrbcbS1'
    },
 */
  //El único grupo que conocemos "group_id": "RX87YY9E"
    grupo: 'RX87YY9E',
